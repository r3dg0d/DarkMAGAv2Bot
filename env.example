# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token_here
CLIENT_ID=your_bot_client_id_here
GUILD_ID=your_guild_id_here

# Discord User Token (Selfbot - Optional)
# Get your token from Discord web app:
# 1. Open Discord in browser and press F12
# 2. Go to Network tab
# 3. Reload Discord and look for 'gateway' request
# 4. Copy the 'authorization' value from headers
# ⚠️ WARNING: Self-bots violate Discord ToS - use at your own risk
SELF_BOT_TOKEN=your_discord_user_token_here

# PayPal API Configuration (Required for payment features)
# See README.md for detailed setup instructions
PAYPAL_CLIENT_ID=your_paypal_client_id_here
PAYPAL_CLIENT_SECRET=your_paypal_client_secret_here
PAYPAL_MERCHANT_EMAIL=your_paypal_business_email@example.com
PAYPAL_MODE=sandbox
# Options: 'sandbox' for testing, 'production' for live payments
PAYPAL_WEBHOOK_ID=your_paypal_webhook_id_here

# PayPal Webhook Server (Optional - if running separate webhook server)
# WEBHOOK_PORT=3000
# WEBHOOK_URL=https://your-domain.com/webhooks

# AI API Keys
# Uncensored AI API Key
# Get your API key from: https://uncensored.ai/
UNCENSORED_API_KEY=your_uncensored_ai_key_here

# Uncensored AI Endpoint (optional, has default)
# UNCENSORED_AI_ENDPOINT=https://mkstqjtsujvcaobdksxs.functions.supabase.co/functions/v1/uncensoredlm-api

# BFL API Key (for image generation)
FLUX_API_KEY=your_bfl_api_key_here

# XAI API Key (for AI chat features)
XAI_API_KEY=your_xai_api_key_here

# Fish Audio API (for voice generation)
FISHAUDIO_API=your_fish_audio_api_key_here

# Server Configuration
MOD_LOG_CHANNEL_ID=your_mod_log_channel_id_here
TICKET_CATEGORY_ID=your_ticket_category_id_here
CHAT_REVIVE_CHANNELS=channel_id1,channel_id2,channel_id3
CHAT_REVIVE_ENABLED=true

